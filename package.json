{
  "name": "jest-coverage-shield",
  "version": "1.0.3",
  "description": "Auto creates jest coverage shield from shield.io",
  "keywords": [
    "coverage",
    "unit test",
    "report",
    "github badge",
    "clover",
    "jest"
  ],
  "license": "MIT",
  "homepage": "https://github.com/patrick-lai/jest-coverage-shield",
  "bugs": {
    "url": "https://github.com/patrick-lai/jest-coverage-shield/issues"
  },
  "files": [
    "lib"
  ],
  "author": {
    "name": "Patrick Lai",
    "url": "https://github.com/patrick-lai"
  },
  "contributors": [
    "jhillacre",
    "patrick-lai"
  ],
  "main": "./lib/index.js",
  "bin": {
    "coverage-shield": "./lib/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:patrick-lai/jest-coverage-shield.git"
  },
  "scripts": {
    "test": "istanbul cover ./node_modules/mocha/bin/_mocha --report text -- -R spec",
    "shield": "node ./lib/cli -s flat-square -d shields/"
  },
  "dependencies": {
    "commander": "^2.8.1",
    "decode-html": "^2.0.0",
    "lodash": "^4.17.10",
    "svg.js": "^2.6.5",
    "svgdom": "0.0.15",
    "xml-splitter": "~1.2.1",
    "xtend": "~4.0.1"
  },
  "devDependencies": {
    "chai": "^3.2.0",
    "codacy-coverage": "1.1.2",
    "coveralls": "2.11.3",
    "istanbul": "0.4.1",
    "jscs": "^2.0.0",
    "mocha": "^2.2.5"
  },
  "licenses": [
    {
      "type": "MIT"
    }
  ]
}
